<h1> Results </h1>
<style>
    h1 {
        color: red;
        font-family: 'Courier New', Courier, monospace;
        font-size: 80px;
        text-align: center;
    }
    
    li {
        text-align: center;
        width: 50%;
        margin: 0 auto;
    }
</style>

<% data["Search"].forEach(function(movie) { %>
    <li>
        <em><%= movie["Title"] %> </em> -
        <%= movie["Year"] %>
    </li>
    <% }) %>